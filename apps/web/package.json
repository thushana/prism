{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint app",
    "format": "prettier --write \"app/**/*.{ts,tsx}\" \"*.{ts,tsx}\" \"*.{js,mjs}\"",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate --config=database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=database/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=database/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=database/drizzle.config.ts",
    "db:seed": "tsx database/seed.ts",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "database": "*",
    "dotenv": "^17.2.3",
    "feature-flags": "*",
    "flags": "^4.0.0",
    "system-sheet": "*",
    "drizzle-orm": "beta",
    "intelligence": "*",
    "logger": "*",
    "next": "16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "ui": "*",
    "@prism/utilities": "*"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "commander": "^12.1.0",
    "drizzle-kit": "beta",
    "@eslint/compat": "^1.2.4",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.1.6",
    "lightningcss": "^1.31.1",
    "postcss-import": "^16.1.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
