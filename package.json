{
  "name": "timetraveler",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf js scripts/*.js *.tsbuildinfo",
    "dev:kill": "lsof -ti:8000 | xargs kill -9 2>/dev/null || true; pkill -f 'tsc --watch' 2>/dev/null || true; pkill -f 'http-server' 2>/dev/null || true; echo 'Killed all dev servers'",
    "dev": "next dev",
    "format:check": "prettier --check \"app/**/*.{ts,tsx}\" \"*.{ts,tsx}\" \"*.{js,mjs}\"",
    "format": "prettier --write \"app/**/*.{ts,tsx}\" \"*.{ts,tsx}\" \"*.{js,mjs}\"",
    "lint:fix": "eslint . --fix",
    "lint": "eslint",
    "precommit": "npm run build",
    "quality": "npm run typecheck && npm run lint && npm run format:check",
    "serve": "http-server -p 8000 -c-1",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "next": "16.0.2"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "16.0.2"
  }
}
