{
  "name": "timetraveler",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf js scripts/*.js *.tsbuildinfo",
    "dev:kill": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true; pkill -f 'tsc --watch' 2>/dev/null || true; pkill -f 'next dev' 2>/dev/null || true; echo 'Killed all dev servers'",
    "dev": "next dev",
    "format:check": "prettier --check \"app/**/*.{ts,tsx}\" \"*.{ts,tsx}\" \"*.{js,mjs}\"",
    "format": "prettier --write \"app/**/*.{ts,tsx}\" \"*.{ts,tsx}\" \"*.{js,mjs}\"",
    "lint:fix": "eslint . --fix",
    "lint": "eslint",
    "precommit": "npm run build",
    "quality": "npm run typecheck && npm run lint && npm run format",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  }
}
